# API Keys Configuration
# Copy this file to .env and add your actual API keys

# Google Gemini API Key (get at: https://makersuite.google.com/app/apikey)
GOOGLE_API_KEY=your_google_api_key_here

# Groq API Key (get at: https://console.groq.com/)
GROQ_API_KEY=your_groq_api_key_here

# Model Configuration
DEFAULT_STT_MODEL=Faster-Whisper - Base
DEFAULT_LLM_PROVIDER=groq

# File Paths
AUDIO_OUTPUT_DIR=./data/audio/output_audio
AUDIO_INPUT_DIR=./data/audio/input_audio

# Server Configuration
SERVER_HOST=127.0.0.1
SERVER_PORT=7860
OPEN_BROWSER=true

# TTS Voice Configuration
TTS_RATE=+12%
TTS_VOLUME=+5%
TTS_PITCH=+0Hz